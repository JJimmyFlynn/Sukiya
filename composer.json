{
  "name": "jjimmyflynn/sukiya",
  "description": "A Craft CMS starter project",
  "authors": [
    {
      "name": "John Flynn",
      "homepage": "https://johnjflynn.net"
    }
  ],
  "require": {
    "craftcms/cms": "3.5",
    "nystudio107/craft-twigpack": "1.2.2",
    "spacecatninja/imager-x": "v3.0.10",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "npx use-preset jjimmyflynn/preset-frontend"
    ]
  }
}
